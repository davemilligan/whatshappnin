<div id='main' class='main'>
<%= render(:partial => "layouts/left", :object => nil) -%>
    <% if @questions %>
        <div id='center' class='center' id=center>
			<%= insert_heading("Feedback", nil) %>   
        <fieldset> 
             <table class="search-table">           
                <tr>
                    <th><%= res("question_th") -%></th>
                    <th><%= res("percent_th") -%></th>
                </tr>                 
                <tr valign="top" class="<%= cycle('list-line-odd',
                                                      'list-line-even') %>">
                    <td width='85%'>
                    	<%= @questions.q1 %>
                    </td> 
					<td>
						<%= get_percent_happy(@questions.id, 'q1')[0].count.to_d * 100 %>

